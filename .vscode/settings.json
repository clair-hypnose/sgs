    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#ceb663",
    "statusBar.background": "#c1a33d",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#9a8231",
    "statusBarItem.remoteBackground": "#c1a33d",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#c1a33d",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#c1a33d99",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "#c1a33d"
